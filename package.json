{
  "name": "@a700/n8n-nodes-agent700",
  "version": "1.0.15",
  "description": "Agent700 AI integration nodes for n8n workflow automation",
  "license": "MIT",
  "homepage": "https://agent700.ai",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "agent700",
    "ai",
    "chat",
    "workflow",
    "automation"
  ],
  "author": {
    "name": "Agent700",
    "email": "support@agent700.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/LevelUpDevelopment/n8n-nodes-agent700.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/Agent700AppPasswordApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Agent700Agent/Agent700Agent.node.js",
      "dist/nodes/Agent700ContextLibrary/Agent700ContextLibrary.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.0.0",
    "eslint": "9.32.0",
    "jest": "^29.5.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "ts-jest": "^29.1.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
